@boardApp = angular
  .module('app.boardApp', [
    'dndLists'
    'toggle-switch'
    'xeditable'
  ])
  .run(->
    console.log 'exampleApp running'
  )
  .run [
    'editableOptions'
    'editableThemes'
    (editableOptions, editableThemes) ->
      editableOptions.theme = 'bs3'
      editableThemes['bs3'].submitTpl = '<button type="submit" class="btn btn-info btn-xs"><span></span></button>'
      editableThemes['bs3'].cancelTpl =   '<button type="button" class="btn btn-xs" ng-click="$form.$cancel()">'+'<span></span>'+'</button>'
      return
  ]